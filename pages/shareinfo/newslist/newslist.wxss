page {
  width: 100%;
  height: 100%;
  background: #fff;
}

.container {
  position: relative;
  width: 100%;
  height: 100%;
  background-color: #fff;
  color: #939393;
}

.nav_left {
  position: fixed;
  width: 25%;
  height: 100%;
  background: #f5f5f5;
  text-align: center;
  background-repeat: no-repeat;
  overflow-x: hidden;
  overflow-y: scroll;
  background-size: cover;
}

.nav_left .nav_left_items {
  height: 30px;
  line-height: 30px;
  padding: 6px 0;
  border-bottom: 1px solid #dedede;
  font-size: 14px;
}

.nav_left .nav_left_items.active {
  background: #fff;
  border-left: 3px solid #129fff;
}

.nav_right {
  position: absolute;
  top: 0;
  right: 0;
  width: 75%;
  height: 100%;
  padding: 10px;
  background: #fff;
  overflow-x: hidden;
  overflow-y: scroll;
  box-sizing: border-box;
}

.weui-media-box_text {
  padding-top: 15rpx;
  padding-bottom: 15rpx;
  border-bottom: 1px solid #dedede;
}

.titlespan {
  font-size: 14px;
  color: #129fff;
  text-decoration: underline;
}
